<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuya.community.business.sdk.demo">

    <application
        android:icon="@mipmap/ic_launcher"
        android:name=".TuyaApplication"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:allowBackup="false"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="TUYA_SMART_APPKEY"
            android:value="${TUYA_SMART_APPKEY}" />
        <meta-data
            android:name="TUYA_SMART_SECRET"
            android:value="${TUYA_SMART_SECRET}" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.QrcodeLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

            </intent-filter>
        </activity>
        <activity android:name=".activity.CommonDeviceDebugActivity"/>
        <activity android:name=".activity.PushActivity"/>
        <activity android:name=".activity.SceneActivity"/>
    </application>

</manifest>